{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/authroute.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/userroute.js"
    },
    {
      "src": "/api/posts/(.*)",
      "dest": "/api/postroute.js"
    },
    {
      "src": "/api/notifications/(.*)",
      "dest": "/api/notification.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
