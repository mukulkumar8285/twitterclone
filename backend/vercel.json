{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/me",
      "dest": "/api/auth/me.js"
    },
    {
      "src": "/api/auth/signup",
      "dest": "/api/auth/signup.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/api/auth/logout",
      "dest": "/api/auth/logout.js"
    },
    {
      "src": "/api/users/profile/:username",
      "dest": "/api/users/profile.js"
    },
    {
      "src": "/api/users/suggested",
      "dest": "/api/users/suggested.js"
    },
    {
      "src": "/api/users/follow/:id",
      "dest": "/api/users/follow.js"
    },
    {
      "src": "/api/users/update",
      "dest": "/api/users/update.js"
    },
    {
      "src": "/api/posts/all",
      "dest": "/api/posts/all.js"
    },
    {
      "src": "/api/posts/following",
      "dest": "/api/posts/following.js"
    },
    {
      "src": "/api/posts/likes/:id",
      "dest": "/api/posts/likes.js"
    },
    {
      "src": "/api/posts/user/:username",
      "dest": "/api/posts/user.js"
    },
    {
      "src": "/api/posts/create",
      "dest": "/api/posts/create.js"
    },
    {
      "src": "/api/posts/like/:id",
      "dest": "/api/posts/like.js"
    },
    {
      "src": "/api/posts/comment/:id",
      "dest": "/api/posts/comment.js"
    },
    {
      "src": "/api/posts/:id",
      "dest": "/api/posts/id.js"
    },
    {
      "src": "/api/notifications/",
      "dest": "/api/notifications/index.js"
    }
  ]
}
